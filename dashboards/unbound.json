      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [
        {"expr": "sum(rate(unbound_num_cachehits_total[5m])) /(sum(rate(unbound_num_cachehits_total[5m])) + sum(rate(unbound_num_cachemiss_total[5m])))", "refId": "A"}
      ],
      "fieldConfig": {"defaults": {"unit": "percentunit"}, "overrides": []}
    },
    {
      "type": "timeseries",
